{
  "name": "rummy-multi-table-node",
  "version": "0.0.1",
  "description": "Pool Rummy Card Game Node.JS application written on Typescript for running the game on multi tables Mode.",
  "main": "index.ts",
  "keywords": [
    "Pool-Rummy"
  ],
  "dependencies": {
    "@newrelic/winston-enricher": "^4.0.1",
    "@socket.io/redis-adapter": "7.0.0",
    "async-redis": "^2.0.0",
    "aws-sdk": "^2.1122.0",
    "bull": "^3.22.4",
    "bullmq": "^5.53.0",
    "chai-as-promised": "^7.1.1",
    "consul": "^0.40.0",
    "copyfiles": "^2.4.1",
    "danger": "^11.2.0",
    "danger-plugin-coverage": "^1.6.2",
    "decimal.js": "^10.4.3",
    "dotenv": "^16.3.1",
    "express": "^4.17.1",
    "firebase-admin": "^12.1.1",
    "fs": "^0.0.1-security",
    "ioredis": "^5.6.1",
    "joi": "^17.6.0",
    "lodash.includes": "^4.3.0",
    "log4js": "^6.9.1",
    "mixpanel": "^0.18.1",
    "mochawesome": "^7.1.3",
    "moment": "^2.29.3",
    "newrelic": "^12.3.1",
    "node-zookeeper-client": "^1.1.1",
    "nyc": "^15.1.0",
    "path": "^0.12.7",
    "pm2": "^5.2.0",
    "redis": "^3.1.2",
    "redlock": "^4.2.0",
    "rimraf": "^5.0.0",
    "shelljs": "^0.8.5",
    "socket.io": "4.1.2",
    "socket.io-client": "^4.5.3",
    "supertest": "^6.2.4",
    "underscore": "^1.13.3",
    "uuid": "^8.3.2",
    "winston": "^3.8.1",
    "winston-loki": "^6.1.3",
    "xstate": "^4.33.6"
  },
  "devDependencies": {
    "@stryker-mutator/api": "^6.2.2",
    "@stryker-mutator/core": "^6.2.2",
    "@stryker-mutator/jest-runner": "^6.2.2",
    "@stryker-mutator/typescript-checker": "^6.2.2",
    "@stryker-mutator/util": "^6.2.2",
    "@types/bull": "^3.15.7",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.5.1",
    "@types/joi": "^17.2.3",
    "@types/mocha": "^9.1.1",
    "@types/node": "^16.11.3",
    "@types/node-zookeeper-client": "^0.2.8",
    "@types/redis": "^2.8.32",
    "@types/redlock": "^4.0.3",
    "@types/shelljs": "^0.8.11",
    "@types/underscore": "^1.11.4",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.57.1",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^7.0.4",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^29.1.2",
    "jest-environment-jsdom-sixteen": "^2.0.0",
    "jsdom": "^20.0.1",
    "prettier": "^3.5.3",
    "prettier-eslint": "^16.3.0",
    "redis-mock": "0.56.3",
    "ts-jest": "^28.0.3",
    "ts-node": "^10.4.0",
    "typescript": "^4.9.5",
    "typescript-eslint-parser": "^18.0.0"
  },
  "peerDependencies": {
    "@stryker-mutator/core": "^6.2.2",
    "typescript": ">=4.4.4"
  },
  "scripts": {
    "build": "npm run clean && tsc && npm run copy-files",
    "clean": "rimraf dist/ stryker-tmp/ stryker.log coverage/ test-jarvis-node-*.log",
    "lint:fix": "eslint 'src/**' --ext .ts --fix",
    "test": "jest --config jest.config.ts",
    "coverage": "jest --coverage --config jest.config.ts --verbose --maxWorkers=50%",
    "test-jest": "jest",
    "test:watch": "jest --watch",
    "copy-files": "copyfiles -u 1 ./src/**/*.proto dist/",
    "start": "npm run build && NODE_ENV=local node dist/app.js",
    "setup:husky": "husky install",
    "mochatest": "mocha --exit --recursive --timeout 1800000 --reporter mochawesome",
    "mochareport": "npm run cleanup && nyc --reporter=html mocha --timeout 1800000",
    "open:report": "open mochawesome-report/mochawesome.html",
    "cleanup": "rimraf coverage/ mochawesome-report/ test-jarvis-node-*.log",
    "test:mutation": "REDIS_PORT=6379 SCHEDULER_PORT=6379 PUB_SUB_REDIS_PORT=6379 GAMEPLAY_REDIS_PORT=6379 PORT=6379 TS_NODE_PROJECT=./tsconfig.json stryker run",
    "mutation:debug": "REDIS_PORT=6379 SCHEDULER_PORT=6379 PUB_SUB_REDIS_PORT=6379 GAMEPLAY_REDIS_PORT=6379 PORT=6379 npx stryker run --fileLogLevel trace --logLevel debug"
  },
  "author": "",
